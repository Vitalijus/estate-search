if ($('.info').length == 0) {
   $("#breed_images").hide(function() {
     $(this).append("<%= j render 'images' %>")
     $(".button").val("Clean");
   })
   $("#breed_images").fadeIn("slow");

} else {
  $(".info").fadeOut("slow", function() {
      $(".button").val("Submit breed");
      $(this).remove();
   });
}
